{
    "authors": [
        {
            "name": "Ernesto G. Liberio Vera",
            "email": "ernesto.liberio@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LaraDevs\\Fcm\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "description": "A package to send Firebase notification cross Laravel Application",
    "extra": {
        "laravel": {
            "providers": [
                "LaraDevs\\Fcm\\FcmServiceProvider"
            ],
            "aliases": {
                "Fcm": "LaraDevs\\Fcm\\FcmFacade"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "name": "laradevs/notifications-fcm",
    "require": {
        "php" : "^5.6|^7.0|^8.0|^8.1|^8.2",
        "illuminate/support": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "LaraDevs\\Fcm\\Test\\": "tests/"
        }
    }
}
